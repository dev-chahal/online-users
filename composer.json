{
    "name": "dev-chahal/online-users",
    "description": "Laravel Package to manage online users.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DevChahal\\OnlineUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevChahal\\OnlineUsers\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Sahil Chahal",
            "email": "dev.sahilchahal@gmail.com"
        }
    ],
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "laravel/pint": "^1.1",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevChahal\\OnlineUsers\\OnlineUsersServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
